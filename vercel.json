{
  "version": 2,
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/images/(.*)", "dest": "/after/images/$1" },
    { "src": "/after/images/(.*)", "dest": "/after/images/$1" },
    { "src": "/(.*\\.html)$", "dest": "/after/$1" },
    { "src": "/(.*\\.css)$", "dest": "/after/$1" },
    { "src": "/(.*\\.js)$", "dest": "/after/$1" },
    { "src": "/(.*\\.png)$", "dest": "/after/$1" },
    { "src": "/(.*\\.jpg)$", "dest": "/after/$1" },
    { "src": "/(.*\\.gif)$", "dest": "/after/$1" },
    { "src": "/.*", "dest": "/after/index.html" }
  ]
}
